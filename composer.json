{
  "name": "kr0lik/php-cs-fixer",
  "type": "library",
  "description": "Code style fixer",
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "bin-dir": "bin"
  },
  "require": {
    "php": ">=7.3.0",
    "friendsofphp/php-cs-fixer": "^2.17"
  },
  "autoload": {
    "psr-4": {
      "kr0lik\\CodeStyleFixer\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
